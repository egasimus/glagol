(fn [s]
  (let [h (.-h @vdom)]
    (h ".samplers"
      (s.samplers.map (fn [sampler i]
        (h ".sampler" { :onclick (fn [] (@sampler-play sampler))
                        :dataset { :key (if (< i 26) (String.from-char-code (+ i 65))) } }
          (.map (sampler.file.split "/") (fn [part]
            (h ".sampler-path-part" part)))))))))
