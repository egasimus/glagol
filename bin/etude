#!/usr/bin/env node

var root =
      require('path').resolve(__dirname, '../server.esl')
  , options =
      { eslisp:
        { transform:
          [ require('eslisp-camelify')
          , require('eslisp-propertify') ] }}
  , app =
      require('glagol').Directory(root, options);

app.tree()['main'](app);
