(lambda (options app)
  (= app (to-glagol app))
  (.for-each (Array.prototype.slice.call arguments 2) overlay)
  (function to-glagol (x)
    (return (?: (=== "string" (typeof x)) (./core/glagol x options) x)))
  (function overlay (x)
    (app.overlay (to-glagol x)))
  (function handler (req res)
    (./core/bundle app req res))
  (= handler.tracked app)
  (return handler))
