((lambda ()

  (var loading ((. (require "path") join) __dirname "loading.js"))
  (= loading ((. (require "fs") read-file-sync) loading "utf8"))
  (var state (object "content" loading))

  (build)
  (return state)

  (function built (result) (= state.content result))
  (function build () (./bundle "./client/web" built))))
