{

  "div#root-box"
  (fn [opts] (./bless/Box (merge
    { :top 0 :bottom 1 :left 1 :right 1 :bg "#111111"} opts)))

  "div.toolbar"
  (fn [] (./bless/Box { :top 0 :left 0 :right 0 :height 3 }))

  "div.toolbar-bpm"
  (fn [] (let
    [box (./bless/Box { :left 0 :width 5 :top 0 :height 3 :color :red :bg "#111111"})]
    (.map
      [ (./bless/Box  { :left 0 :right 0 :top 0 :height 1 :ch "▄" :bg nil :fg "#111111"})
        (./bless/Text { :left 1 :right 1 :top 1 :height 1 :bg "#111111" :fg :yellow :content "174"})
        (./bless/Box  { :left 0 :right 0 :top 2 :height 1 :ch "▀" :bg nil :fg "#111111"}) ]
      (box.append.bind box) )
    box))

  "div.toolbar-tap"
  (fn [] (let
    [box (./bless/Box { :left 6 :width 5 :top 0 :height 3 :color :red :bg "#111111"})]
    (.map
      [ (./bless/Box  { :left 0 :right 0 :top 0 :height 1 :ch "▄" :bg nil :fg "#111111"})
        (./bless/Text { :left 1 :right 1 :top 1 :height 1 :bg "#111111" :fg :yellow :content "BPM"})
        (./bless/Box  { :left 0 :right 0 :top 2 :height 1 :ch "▀" :bg nil :fg "#111111"}) ]
      (box.append.bind box) )
    box))

  "div.toolbar-metre"
  (fn [] (let
    [box (./bless/Box { :left 14 :width 5 :top 0 :height 3 :color :red :bg "#111111"})]
