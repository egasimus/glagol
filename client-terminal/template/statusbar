(fn template-statusbar [state]
  ["div.statusbar"
    { :tags true :bg :black :fg :yellow
      :left 1 :right 1 :bottom 0 :height 1 } 
    ["div.text"
      (.reduce (keys ../options/keymap) (fn [a b]
        (str a "foo {red-bg}" (.join (aget ../options/keymap b) "{/red-bg} {red-bg}")
               "{/red-bg} {black-bg}" b "{/black-bg}  ")) "")]])
