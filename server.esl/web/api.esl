(object
  "root"
    (lambda (callback)
      (try
        (var ice)
        (= ice (./client/freeze))
        (catch e (console.error "Error freezing client:\n" e.stack)))
      (try
        (var deps)
        (= deps (./deps ./client))
        (console.log "deps" deps)
        (catch e (console.error "Error bundling client deps:\n" e.stack)))
      (callback (JSON.stringify ice))))
