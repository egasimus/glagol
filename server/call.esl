(lambda (path args)
  (console.log "\n=>" path args)
  (return (call1 (path.split "/") ./api))
  (function call1 (p q)
    (console.log "call1" p q)
    (if (> 1 p.length)
      (throw (Error "api: specify path"))
      (return (?: (< 1 p.length)
        (call1 (p.slice 1) (get q (. p 0)))
        (console.log ">>" q))))))
