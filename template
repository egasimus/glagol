(fn [s]
  (let [h (.-h @vdom)]
    (h ".app" [
      (@template-toolbar  s)
      (h ".main" [
        (@template-search   s)
        (h ".flex.col" [
          (@template-sequencer :kick  s.kick.length)
          (@template-sequencer :snare s.kick.length)
          (@template-samplers s)
        ])
      ])
    ])))
