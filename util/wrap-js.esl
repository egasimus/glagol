(lambda wrap-js (bundle app cb)
  (.read-file (require "fs")
    (.resolve (require "path") __dirname ".." "client" "boot.js")
    "utf-8"
    (lambda (err code)
      (if err (throw err))
      (= code (code.replace "%REQUIRE%" (./just bundle)))
      (= code (code.replace "%DEPS%"    (./just (JSON.stringify app.deps.deps))))
      (= code (code.replace "%ICE%"     (./just (JSON.stringify app.ice))))
      (cb code))))
