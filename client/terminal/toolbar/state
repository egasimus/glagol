(let [state { :jack-up false
              :playing false }]

  (-> (../api :invoke "jack/start")
    (.then (fn []
      (set! state.jack-up true)
      (../render))))

  (../events/on :play (fn []
    (set! state.playing (not state.playing))
    (../render)))

    ;(-> ../server/api
      ;(.get "sequencer")
      ;(.invoke "jack-up")
      ;(.then (fn []
        ;(set! state.jack-up true)
        ;(./render ../screen state))))))

  state)
