.Terminal
  width 640px
  margin 0 12px 6px 0
  margin-bottom 6px

  .terminal
    height 300px
    font-family monospace
    font-size 10px
    color #aaa
    background rgba(0,0,0,0.5)
    margin 0 6px
    padding 12px 9px
    line-height 10px
    &:focus
      outline 1px solid rgba(255,255,255,0.2)
