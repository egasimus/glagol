(let [state { :jack-up false }]

  (../server/socket/on :open (fn []
    (../api :invoke "sequencer/jack-up" (fn []
      (set! state.jack-up true)
      (./render ../screen state)))))

    ;(-> ../server/api
      ;(.get "sequencer")
      ;(.invoke "jack-up")
      ;(.then (fn []
        ;(set! state.jack-up true)
        ;(./render ../screen state))))))

  state)
