  "div.statusbar"
  (fn [] (let
    [box (./bless/Box { :bottom 1 :height 1 :left 1 :right 1 })]
    (box.append (./bless/Text { :content "foo" :left 0 :right 0 :top 1 :height 1 }))
    box))

  "div.text"
  (fn [] (./bless/Box))

  "div.session"
  (fn [] (./bless/Box))

