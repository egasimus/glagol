(fn soxi-analyze [file]
  (let [data
          (String (.exec-file-sync (require :child_process)
            ./options/soxi-path [file]))
        info
          {}]

    (.map (keys ./soxi-regex)
      (fn [param]
        (let [regex (aget ./soxi-regex param)]
          (aset info param (aget (regex.exec data) 1)))))

    info))
